  "preloadRule": {
    "pages-home/pages/module/index": {
      "network": "all",
      "packages": ["__APP__"]
    },
    "pages/classify/index": {
      "network": "all",
      "packages": ["pages-classify"]
    },
    "pages/commodity/index": {
      "network": "all",
      "packages": ["pages-commodity"]
    },
    "pages/order/index": {
      "network": "all",
      "packages": ["pages-order"]
    },
    "pages-mine/pages/about/index": {
      "network": "all",
      "packages": ["pages-mine"]
    }
